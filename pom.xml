<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>miven.springboot</groupId>
  <artifactId>springboot-learning</artifactId>
  <version>1.0-SNAPSHOT</version>
  <packaging>pom</packaging>

  <properties>
      <java.version>1.8</java.version>
      <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
      <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>

      <springboot.version>2.1.6.RELEASE</springboot.version>

      <alibaba.fastjson.version>1.2.56</alibaba.fastjson.version>
      <lombok.version>1.18.6</lombok.version>
      <miven.version>1.0</miven.version>
      <mybatis.generator.version>1.3.7</mybatis.generator.version>
      <mysql.connector.version>5.1.46</mysql.connector.version>
      <pagehelper.starter.version>1.2.10</pagehelper.starter.version>
      <tk.mapper.version>4.1.5</tk.mapper.version>
      <tk.mapper.starter.version>2.1.5</tk.mapper.starter.version>
      <tk.mapper.generator.version>1.1.5</tk.mapper.generator.version>
  </properties>

  <modules>
      <module>springboot-demo</module>
      <module>springboot-redis</module>
      <module>springboot-mybatis</module>
      <module>springboot-mongodb</module>
  </modules>

  <dependencyManagement>
      <dependencies>
            <dependency>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-starter-parent</artifactId>
                <version>${springboot.version}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
      </dependencies>
  </dependencyManagement>

  <build>
      <pluginManagement>
          <plugins>
              <plugin>
                  <groupId>org.springframework.boot</groupId>
                  <artifactId>spring-boot-maven-plugin</artifactId>
                  <version>${springboot.version}</version>
                  <executions>
                      <execution>
                          <goals>
                              <goal>repackage</goal>
                              <goal>build-info</goal>
                          </goals>
                      </execution>
                  </executions>
              </plugin>
          </plugins>
      </pluginManagement>
  </build>
</project>